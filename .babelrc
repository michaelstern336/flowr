{
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": "> 1%, last 2 versions"
        },
        "modules": false
      }
    ]
  ],
  "plugins": ["transform-class-properties", "transform-object-rest-spread"],
  "env": {
    "cjs": {
      "plugins": [["transform-es2015-modules-commonjs", { "loose": true }]]
    },
  }
}
